<h1> Create a new Event </h1>
<br>
<%= form_for @event ,html:{"data-turbo"=>false} do |form| %>
  <div>
    <%= form.label :eventname %><br>
    <%= form.text_field :eventname %>
    
  </div>

  <div>
    <%= form.label :date %><br>
    <%= form.text_area :date %><br>
    
  </div>

  <div>
    <%= form.submit %>
  </div>


<% end %>